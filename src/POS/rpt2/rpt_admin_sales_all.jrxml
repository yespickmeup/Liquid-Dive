	<queryString>
		<![CDATA[select 
  r.id                
 ,r.cashier_session_id
 ,r.tendered          
 ,r.or_number         
 ,r.receipt_status    
 ,r.receipt_date      
          
 ,ri.receipt_id  
 ,ri.product_name
 ,ri.unit_price  
 ,ri.qty         

 ,cs.cashier_name

 ,cd.cash_in
 ,(cd.cash_out + cd.addtl_cash) as cashout
from db_pos_meatshop.cashier_sessions cs
  join db_pos_meatshop.receipts r
  on cs.id = r.cashier_session_id
  join db_pos_meatshop.receipt_items ri
  on r.id = ri.receipt_id
  left join db_pos_meatshop.cash_drawer cd
  on cs.id = cd.cashier_session_id
where date(receipt_date) 
  between date($P{date_from}) and date($P{date_to});]]>
	</queryString>
