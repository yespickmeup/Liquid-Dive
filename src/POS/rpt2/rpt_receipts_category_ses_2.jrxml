		<![CDATA[select 
 ri.receipt_id 
,ri.product_name 
,ri.unit_price 
,ri.qty
,ri.type_name
,ri.description

,r.receipt_date
,r.cashier_session_id
,(r.tendered-r.credit) as tendered
,r.or_number
,r.receipt_status
,r.receipt_date
,r.discount
,r.credit
,r.approval_code

,cs.cashier_name
 ,cs.login_time
 ,cs.logout_time

 ,cd.cashier_session_id
 ,cd.user_name
 ,cd.date_added
 ,cd.cash_in
 ,ifnull(cd.cash_out,0) as cash_out
 ,cd.addtl_cash

from "+MyDB.getNames()+".receipt_items ri
join "+MyDB.getNames()+".receipts r
on ri.receipt_id=r.or_number
join "+MyDB.getNames()+".cashier_sessions cs
on cs.id=r.cashier_session_id
join "+MyDB.getNames()+".cash_drawer cd
on cd.cashier_session_id=cs.id
where date(r.receipt_date) between date($P{date_from}) and date($P{date_to})]]>
	</queryString>
