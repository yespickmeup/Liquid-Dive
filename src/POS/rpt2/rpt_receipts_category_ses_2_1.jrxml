	<queryString>
		<![CDATA[select 
 ri.receipt_id 
,ri.product_name 
,ri.unit_price 
,sum(ri.qty) as qty
,ri.type_name
,ri.description

,r.receipt_date
,r.cashier_session_id
,(r.tendered-r.credit) as tendered
,r.or_number
,r.receipt_status
,r.receipt_date
,r.discount
,r.credit
,r.approval_code


from db_pos_restaurant.receipt_items ri
join db_pos_restaurant.receipts r
on ri.receipt_id=r.or_number
where date(r.receipt_date) between date($P{date_from}) and date($P{date_to})
group by ri.product_name]]>
	</queryString>
